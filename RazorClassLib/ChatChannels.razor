@using MudBlazor
@using Dovecord.Shared
<MudList Clickable="true">
    <MudListItem Text="Information" InitiallyExpanded="true" Dense="true" Class="mt-4">
        <NestedList>
            
            @{
                foreach (var channel in Channels)
                {
                    <MudListItem Dense="true"># @channel.ChannelName</MudListItem>
                }
            }
        </NestedList>
    </MudListItem>
</MudList>

@code {
    [Parameter] public List<Channel> Channels { get; set; }
}